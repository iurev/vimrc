"-----------------------------------------------------------
" Плагины
source ~/.vim/conf/plugins.vim
"-----------------------------------------------------------

"-----------------------------------------------------------
" Статическое, малоизменяемое поведение
source ~/.vim/conf/config.vim
"-----------------------------------------------------------

"-----------------------------------------------------------
" Словари
source ~/.vim/conf/dicts.vim
"-----------------------------------------------------------

"-----------------------------------------------------------
" Внешний вид
source ~/.vim/conf/gui.vim
"-----------------------------------------------------------

"-----------------------------------------------------------
" Динамическое поведение
source ~/.vim/conf/motion.vim
"-----------------------------------------------------------
